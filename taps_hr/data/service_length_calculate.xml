<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    <record id="service_length_calculate" model="base.automation">
        <field name="name">Service Length Calculate</field>
        <field name="model_id" ref="model_hr_employee"/>
        <field name="state">code</field>
        <field name="code">model._calculate_serviceLength(record.emp_id,record.joining_date,record.resign_date)</field>
        <field name="trigger">on_create_or_write</field>
        <field name="trigger_field_ids" eval="[(4, ref('hr.field_hr_employee____last_update'))]"/>
        <field name="active" eval="True"/>
    </record>
    </data>
</odoo>