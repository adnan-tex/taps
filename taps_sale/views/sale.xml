<odoo>
  <data>
      <record id="sale_order_varient_columns" model="ir.ui.view">
          <field name="name">sale.order.lineview</field>
          <field name="model">sale.order</field>
          <field name="inherit_id" ref="sale.view_order_form"/>
          <field name="arch" type="xml">  
              <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='order_lines']/field[@name='order_line']/tree[1]/field[@name='name']" position="after">
                  <field name="dippingfinish"/>
                  <field name="slidercodesfg"/>
                  <field name="ptopfinish"/>
                  <field name="pbotomfinish"/>
                  <field name="topbottom"/>
                  <field name="gap"/>
                  <field name="sizein"/>
                  <field name="sizecm"/>
                  <field name="sizemm"/>
                  <field name="finish"/>
                  <field name="shade"/>
              </xpath>
          </field>
      </record>
  </data>
</odoo>